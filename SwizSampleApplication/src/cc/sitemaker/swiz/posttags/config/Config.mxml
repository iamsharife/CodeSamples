<?xml version="1.0" encoding="utf-8"?>
<swiz:Swiz xmlns:fx="http://ns.adobe.com/mxml/2009" 
		   xmlns:swiz="http://swiz.swizframework.org" 
		   xmlns:config="cc.sitemaker.swiz.posttags.config.*">
	
	<swiz:beanProviders>
		<config:Beans />
	</swiz:beanProviders>
	
	<swiz:loggingTargets>
		<swiz:SwizTraceTarget id="myTraceTarget" />
	</swiz:loggingTargets>
	
	<swiz:config>
		<!-- The eventPackages value tells Swiz the path to your Event classes,
		and viewPackages is an optional value that speeds up the processing of display classes. -->
		<swiz:SwizConfig 
			eventPackages="cc.sitemaker.swiz.posttags.events.*"
			viewPackages="cc.sitemaker.swiz.posttags.views.*"/>
	</swiz:config>
	
	
</swiz:Swiz>
